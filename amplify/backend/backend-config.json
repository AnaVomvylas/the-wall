{
  "auth": {
    "theWallAuth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "storage": {
    "postsTable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "postsFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "postsTable",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        },
        {
          "category": "auth",
          "resourceName": "theWallAuth",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "storage",
          "resourceName": "postsTable",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "api": {
    "theWallApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "postsFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}